{
   "compilerOptions": {
      "lib": [
         "es6",
         "es2017",
         "DOM"
      ],
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,

      "removeComments": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "noImplicitThis": true,
      "resolveJsonModule": true,
      "noImplicitAny": false
      },
   "exclude": ["node_modules"],
   "include": ["./src", "src/entities/Instructions/.ts"]
}